[
    {
       "title":"@@form.cardanoaccountloadbinder.header.config@@",
       "properties":[
            {
                "name":"networkType",
                "label":"@@cardano.prop.networkType@@",
                "description":"@@cardano.prop.networkType.readonly.desc@@",
                "type":"selectbox",
                "options" : [{
                    "value" : "mainnet",
                    "label" : "@@cardano.prop.networkType.mainnet@@"
                },{
                    "value" : "testnet",
                    "label" : "@@cardano.prop.networkType.testnet@@"
                }],
                "value":"testnet"
            },
            {
                "name":"backendService",
                "label":"@@cardano.prop.backendService@@",
                "type":"selectbox",
                "options" : [
                    {
                        "value" : "blockfrost",
                        "label" : "@@cardano.prop.backendService.blockfrost@@"
                    },
                    {
                        "value" : "dandelion",
                        "label" : "@@cardano.prop.backendService.dandelion@@"
                    },
                    {
                        "value" : "customGraphQl",
                        "label" : "@@cardano.prop.backendService.customGraphql@@"
                    }
                ],
                "value":"dandelion"
            },
            {
                "name":"blockfrostProjectKey",
                "label":"@@cardano.prop.blockfrostProjectKey@@",
                "description":"@@cardano.prop.blockfrostProjectKey.desc@@",
                "type":"textfield",
                "required":"True",
                "control_field":"backendService",
                "control_value":"blockfrost",
                "control_use_regex":"false"
            },
            {
                "name":"graphqlEndpointUrl",
                "label":"@@cardano.prop.graphqlEndpointUrl@@",
                "type":"textfield",
                "required":"True",
                "control_field":"backendService",
                "control_value":"customGraphQl",
                "control_use_regex":"false"
            },
            {
                "name":"accountAddress",
                "label":"@@form.cardanoaccountloadbinder.accountAddress@@",
                "description":"@@form.cardanoaccountloadbinder.accountAddress.desc@@",
                "type":"textfield",
                "required":"True"
            }
        ]
    },
    {
        "title":"@@form.cardanoaccountloadbinder.header.mapToFields@@",
        "properties":[
            {
                "name":"balanceField",
                "label":"@@form.cardanoaccountloadbinder.mapToFields.balanceField@@",
                "type":"textfield",
                "required":"True"
            },
            {
                "name":"accountType",
                "label":"@@form.cardanoaccountloadbinder.mapToFields.accountType@@",
                "type":"textfield"
            }
        ]
    }
]
