[
    {
       "title":"@@form.cardanometadataloadbinder.header.config@@",
       "properties":[
            {
                "name":"networkType",
                "label":"@@cardano.prop.networkType@@",
                "description":"@@form.cardanometadataloadbinder.networkType.desc@@",
                "type":"selectbox",
                "options" : [{
                    "value" : "mainnet",
                    "label" : "@@cardano.prop.networkType.mainnet@@"
                },{
                    "value" : "testnet",
                    "label" : "@@cardano.prop.networkType.testnet@@"
                }],
                "value":"testnet"
            },
            {
                "name":"backendService",
                "label":"@@cardano.prop.backendService@@",
                "type":"selectbox",
                "options" : [
                    {
                        "value" : "blockfrost",
                        "label" : "@@cardano.prop.backendService.blockfrost@@"
                    },
                    {
                        "value" : "dandelion",
                        "label" : "@@cardano.prop.backendService.dandelion@@"
                    },
                    {
                        "value" : "customGraphQl",
                        "label" : "@@cardano.prop.backendService.customGraphql@@"
                    }
                ],
                "value":"dandelion"
            },
            {
                "name":"blockfrostProjectKey",
                "label":"@@cardano.prop.blockfrostProjectKey@@",
                "description":"@@cardano.prop.blockfrostProjectKey.desc@@",
                "type":"textfield",
                "required":"True",
                "control_field":"backendService",
                "control_value":"blockfrost",
                "control_use_regex":"false"
            },
            {
                "name":"graphqlEndpointUrl",
                "label":"@@cardano.prop.graphqlEndpointUrl@@",
                "type":"textfield",
                "required":"True",
                "control_field":"backendService",
                "control_value":"customGraphQl",
                "control_use_regex":"false"
            },
            {
                "name":"transactionId",
                "label":"@@form.cardanometadataloadbinder.transactionId@@",
                "description":"@@form.cardanometadataloadbinder.transactionId.desc@@",
                "type":"textfield",
                "required":"True"
            }
        ]
    },
    {
        "title":"@@form.cardanometadataloadbinder.header.mapToFields@@",
        "properties":[
            {
                "name":"metadata",
                "label":"@@process.cardanometadataloadbinder.metadata@@",
                "type":"grid",
                "columns":[
                    {
                        "key":"metadataField",
                        "label":"@@process.cardanometadataloadbinder.metadata.metadataField@@"
                    },
                    {
                        "key":"formFieldId",
                        "label":"@@process.cardanometadataloadbinder.metadata.formFieldId@@"
                    }
                ],
                "required":"True"
            }
        ]
    }
]
